{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["dom", "dom.iterable", "esnext", "es2015"],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "types": ["react"],
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "strict": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}